    <script>
        function checkPassword() {
            const passwordInput = document.getElementById('password');
            const errorMsg = document.getElementById('error-msg');

            if (passwordInput.value.trim() === 'admin') {
                document.getElementById('login-view').classList.add('hidden');
                document.getElementById('app-container').classList.remove('hidden');
            } else {
                errorMsg.classList.remove('hidden');
                passwordInput.classList.add('border-red-500', 'ring-1', 'ring-red-500');
                passwordInput.focus();
            }
        }

        document.addEventListener('DOMContentLoaded', () => {
            const loginBtn = document.getElementById('login-btn');
            const passwordInput = document.getElementById('password');

            if (loginBtn) {
                loginBtn.addEventListener('click', checkPassword);
            }

            if (passwordInput) {
                passwordInput.addEventListener('keypress', (e) => {
                    if (e.key === 'Enter') {
                        checkPassword();
                    }
                });

                passwordInput.addEventListener('input', () => {
                    document.getElementById('error-msg').classList.add('hidden');
                    passwordInput.classList.remove('border-red-500', 'ring-1', 'ring-red-500');
                });
            }

            // Initialize all rendering
            renderTools();
            renderAI();
            renderDedicated('geo-container', geoData);
            renderDedicated('media-container', mediaData);
            renderDedicated('archives-container', archivesData);
            renderDedicated('phone-container', phoneData);
            renderDedicated('resources-container', resourcesData);
        });

        function switchView(view) {
            const views = ['tools', 'guide', 'geo', 'media', 'archives', 'phone', 'resources', 'ai', 'osint'];
            views.forEach(v => {
                const el = document.getElementById(`${v}-view`);
                if (el) el.classList.add('hidden');
            });
            const target = document.getElementById(`${view}-view`);
            if (target) target.classList.remove('hidden');

            const titles = {
                tools: 'OSINT Tool List',
                guide: 'Recon Guide',
                geo: 'Geolocation & Maps',
                media: 'Images & Videos',
                archives: 'Archives & History',
                phone: 'Phone & Messaging',
                resources: 'Downloads / Resources',
                ai: 'AI Hub',
                osint: 'OSINT Framework'
            };
            document.getElementById('view-title').innerText = titles[view] || 'OSINT Tool List';
            toggleMenu();
        }

        function toggleMenu() {
            const sideNav = document.getElementById('side-nav');
            const navContent = document.getElementById('nav-content');
            if (sideNav.classList.contains('hidden')) {
                sideNav.classList.remove('hidden');
                setTimeout(() => navContent.classList.remove('hidden-nav'), 10);
            } else {
                navContent.classList.add('hidden-nav');
                setTimeout(() => sideNav.classList.add('hidden'), 300);
            }
        }

        document.getElementById('menu-btn').addEventListener('click', toggleMenu);
        document.getElementById('nav-backdrop').addEventListener('click', toggleMenu);

