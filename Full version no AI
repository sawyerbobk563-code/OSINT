        function renderTools(filter = '') {
            const container = document.getElementById('tools-container');
            container.innerHTML = '';
            const lowerFilter = filter.toLowerCase();
            toolsData.forEach(section => {
                const filteredItems = section.items.filter(item =>
                    item.name.toLowerCase().includes(lowerFilter) ||
                    item.desc.toLowerCase().includes(lowerFilter)
                );
                if (filteredItems.length > 0) {
                    const sectionEl = document.createElement('section');
                    let html = `<h2 class="text-lg font-black bg-gray-100 p-1 mb-3 uppercase tracking-tight">${section.category}</h2>`;
                    html += `<ul class="space-y-4">`;
                    filteredItems.forEach(tool => {
                        html += `
                            <li class="border-l-4 border-gray-300 pl-4">
                                <a href="${tool.url}" target="_blank" rel="noopener noreferrer" class="text-blue-700 font-bold underline hover:text-blue-900">
                                    ${tool.name}
                                </a>
                                <p class="text-sm text-gray-700 mt-1">${tool.desc}</p>
                                <p class="text-xs text-gray-400 font-mono mt-1">${tool.url}</p>
                            </li>
                        `;
                    });
                    html += `</ul>`;
                    sectionEl.innerHTML = html;
                    container.appendChild(sectionEl);
                }
            });
        }

        function renderAI() {
            const container = document.getElementById('ai-container');
            container.innerHTML = '';
            aiData.forEach(section => {
                let html = `<section><h3 class="text-lg font-black bg-purple-50 p-1 mb-3 uppercase tracking-tight text-purple-800">${section.category}</h3><div class="grid grid-cols-1 md:grid-cols-2 gap-4 border-l-4 border-purple-200 pl-4">`;
                section.items.forEach(i => html += `<div class="p-2 border border-gray-100 rounded hover:bg-gray-50"><a href="${i.url}" target="_blank" class="text-blue-700 hover:underline font-medium">${i.name}</a><p class="text-xs text-gray-400 truncate">${i.url}</p></div>`);
                container.innerHTML += html + `</div></section>`;
            });
        }

        function renderDedicated(containerId, data) {
            const container = document.getElementById(containerId);
            container.innerHTML = '';
            data.forEach(section => {
                let html = `<section><h3 class="text-lg font-black bg-green-50 p-1 mb-3 uppercase tracking-tight text-green-800">${section.category}</h3><div class="grid grid-cols-1 md:grid-cols-2 gap-4 border-l-4 border-green-200 pl-4">`;
                section.items.forEach(i => html += `<div class="p-2 border border-gray-100 rounded hover:bg-gray-50"><a href="${i.url}" target="_blank" class="text-blue-700 hover:underline font-medium">${i.name}</a><p class="text-xs text-gray-400 truncate">${i.url}</p><p class="text-sm text-gray-700 mt-1">${i.desc}</p></div>`);
                container.innerHTML += html + `</div></section>`;
            });
        }

        function filterTools() {
            renderTools(document.getElementById('search').value);
        }
    </script>
</body>
</html>
