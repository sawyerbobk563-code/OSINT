        function renderTools(filter = '') {
            const container = document.getElementById('tools-container');
            container.innerHTML = '';
            toolsData.forEach(section => {
                const items = section.items.filter(i => i.name.toLowerCase().includes(filter.toLowerCase()));
                if (items.length) {
                    let html = `<section><h2 class="text-lg font-black bg-gray-100 p-1 mb-3 uppercase">${section.category}</h2><ul class="space-y-4">`;
                    items.forEach(i => html += `<li class="border-l-4 border-gray-300 pl-4"><a href="${i.url}" target="_blank" class="text-blue-700 font-bold underline">${i.name}</a><p class="text-sm text-gray-700">${i.desc}</p></li>`);
                    container.innerHTML += html + `</ul></section>`;
                }
            });
        }

        function renderAI() {
            const container = document.getElementById('ai-container');
            container.innerHTML = '';
            aiData.forEach(section => {
                let html = `<section><h3 class="text-lg font-black bg-purple-50 p-1 mb-3 uppercase tracking-tight text-purple-800">${section.category}</h3><div class="grid grid-cols-1 md:grid-cols-2 gap-4 border-l-4 border-purple-200 pl-4">`;
                section.items.forEach(i => html += `<div class="p-2 border border-gray-100 rounded hover:bg-gray-50"><a href="${i.url}" target="_blank" class="text-blue-700 hover:underline font-medium">${i.name}</a><p class="text-xs text-gray-400 truncate">${i.url}</p></div>`);
                container.innerHTML += html + `</div></section>`;
            });
        }

        function filterTools() { renderTools(document.getElementById('search').value); }

        renderTools();
        renderAI();
    </script>
